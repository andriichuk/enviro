{
    "name": "andriichuk/env-spec",
    "description": "PHP dot env file manager based on specification.",
    "keywords": [
        "env",
        "env-spec",
        "env-manager"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Andriichuk\\Enviro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andriichuk\\Enviro\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Serhii Andriichuk",
            "email": "andriichuk29@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^v5.4",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.16",
        "mikey179/vfsstream": "1.6.10",
        "symfony/dotenv": "^5.4",
        "vlucas/phpdotenv": "^5.4"
    }
}
