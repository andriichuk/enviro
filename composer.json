{
    "name": "andriichuk/keepenv",
    "description": "KeepEnv is a tool for checking and managing environment variables based on a specification file.",
    "keywords": [
        "dotenv", "env", "env-spec",  "env-manager", "env-validation"
    ],
    "homepage": "https://github.com/andriichuk/keepenv",
    "authors": [
        {
            "name": "Serhii Andriichuk",
            "email": "andriichuk29@gmail.com",
            "homepage": "https://github.com/andriichuk",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Andriichuk\\KeepEnv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andriichuk\\KeepEnv\\": "tests"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-mbstring": "*",
        "ext-json": "*",
        "symfony/yaml": "^5.4",
        "symfony/console": "^5.0 || ^6.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^v5.4",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.16",
        "mikey179/vfsstream": "1.6.10",
        "symfony/dotenv": "^5.4",
        "vlucas/phpdotenv": "^5.4",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/keepenv"
    ]
}
